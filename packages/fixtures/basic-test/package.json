{
  "name": "@abstractest/fixture-basic-test",
  "description": "Testing fixtures",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "private": true,
  "scripts": {
    "build": "exit 0",
    "test": "concurrently 'npm:test:*'",
    "test:native": "abstractest --runner=native src/test/ts/**/*",
    "test:jest": "abstractest --runner=jest src/test/ts/**/*"
  },
  "devDependencies": {
    "@abstractest/infra": "workspace:*",
    "@abstractest/jest": "workspace:*",
    "@abstractest/native": "workspace:*",
    "abstractest": "workspace:*"
  }
}
